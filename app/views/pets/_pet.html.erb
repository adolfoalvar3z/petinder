<div id="<%= dom_id pet %>">
  <p>
    <strong>Name:</strong>
    <%= pet.name %>
  </p>

  <p>
    <strong>Race:</strong>
    <%= pet.race %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= pet.age %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= pet.gender %>
  </p>

  <p>
    <strong>Item:</strong>
    <%= pet.item %>
  </p>

  <p>
    <strong>User:</strong>
    <%= pet.user_id %>
  </p>

  <p>
    <strong>Imagen:</strong>
    <p>
<% if pet.images.attached? %>
  <% pet.images.each do |imagen|%>
    <p>
<div class="row">
  <div class="col-4">
<div class="card text-start">
    <%= image_tag imagen, class: 'card-img-top', width: "10" %>
</div>
  </div>
</div>
    </p>
  <% end %>
<% end %>
    </p>

  </p>

</div>
